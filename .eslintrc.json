{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true
	},
	"extends": "standard",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"semi": ["warn", "always"],
		"max-len": ["error", { "code": 140, "comments": 250 }],
		"no-undef": "off",
		"no-tabs": "off",
		"indent": "off"
	}
}
